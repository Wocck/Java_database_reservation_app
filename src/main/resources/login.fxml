        <Label fx:id="dbStatus" layoutX="125.0" layoutY="15.0" prefHeight="27.0" prefWidth="178.0" />
        <TextField fx:id="username" layoutX="125.0" layoutY="92.0" prefHeight="39.0" prefWidth="247.0" />
        <Label layoutX="30.0" layoutY="98.0" text="Username" />
        <PasswordField fx:id="password" layoutX="125.0" layoutY="150.0" prefHeight="39.0" prefWidth="247.0" />
        <Label layoutX="30.0" layoutY="156.0" text="Password" />
        <ComboBox fx:id="comboBox" layoutX="125.0" layoutY="200.0" prefHeight="39.0" prefWidth="247.0" promptText="Select User Type" />
